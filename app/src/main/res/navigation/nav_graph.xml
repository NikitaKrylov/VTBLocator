<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mapFragment">

    <fragment
        android:id="@+id/mapFragment"
        android:name="com.example.misisvtbhack.MapFragment"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map" >
        <action
            android:id="@+id/action_mapFragment_to_branchDetailFragment"
            app:destination="@id/branchDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/branchDetailFragment"
        android:name="com.example.misisvtbhack.BranchDetailFragment"
        android:label="fragment_branch_detail"
        tools:layout="@layout/fragment_branch_detail" >
        <action
            android:id="@+id/action_branchDetailFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
    </fragment>
</navigation>